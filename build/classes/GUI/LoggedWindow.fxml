<AnchorPane id="AnchorPane" prefHeight="426.0" prefWidth="703.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LoggedWindowController">
   <children>
      <ImageView fitHeight="446.0" fitWidth="703.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/background_theme.jpg" />
         </image>
      </ImageView>
      <Label layoutX="175.0" layoutY="14.0" text="Přehled aktuálních představení" textFill="WHITE">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Separator layoutX="121.0" orientation="VERTICAL" prefHeight="707.0" prefWidth="25.0" />
      <MenuButton layoutX="8.0" layoutY="61.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="117.0" text="Menu">
        <items>
          <MenuItem mnemonicParsing="false" text="Správa lístků" />
          <MenuItem fx:id="logOutBtn" mnemonicParsing="false" onAction="#LogOut" text="Odhlášení" />
        </items>
      </MenuButton>
      <TableView fx:id="eventsTable" layoutX="146.0" layoutY="74.0" prefHeight="305.0" prefWidth="416.0">
        <columns>
          <TableColumn fx:id="nazevColumn" prefWidth="197.0" text="Název" />
          <TableColumn fx:id="timeColumn" prefWidth="219.0" text="Čas představení" />
        </columns>
      </TableView>
      <Button layoutX="601.0" layoutY="114.0" mnemonicParsing="false" onAction="#showDetails" prefHeight="31.0" prefWidth="102.0" text="Podrobnosti" wrapText="true" />
      <Button layoutX="601.0" layoutY="74.0" mnemonicParsing="false" onAction="#loadEventsFromDatabase" text="Aktualizovat" />
   </children>
</AnchorPane>
